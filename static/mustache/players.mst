<table>
	<tr>
		<th>Name</th>
		<th colspan="2" >Country</th>
		<th>Association</th>
		<th>Pool</th>
	</tr>
	{{ #players }}
		<tr class="player" data-id="{{ id }}">
			<td class="name">{{ name }}</td>
			<td class="country">{{ country }}</td>
			<td class="flag">{{{ flag_image }}}</td>
			<td class="association">{{ association }}</td>
			<td class="pool">{{ pool }}</td>
		</tr>
	{{ /players }}
</table>
